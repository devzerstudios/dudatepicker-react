{
  "name": "dudatepicker-react",
  "version": "0.1.0",
  "description": "React + TS wrapper/components for duDatepicker with Tailwind/shadcn styling",
  "license": "MIT",
  "author": "Your Name",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
