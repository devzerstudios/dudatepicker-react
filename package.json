    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "datepicker",
    "react",
    "typescript",
    "tailwind",
    "shadcn",
    "nextjs",
    "duDatepicker"
